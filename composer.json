{
  "name": "encreinformatique/expertsender-api-php",
  "description": "A PHP wrapper for the ExpertSender API",
  "type": "library",
  "authors": [
    {
      "name": "Isaac Rozas",
      "email": "contact@expertsender.fr"
    },
    {
      "name": "Julien Devergnies",
      "email": "j.devergnies@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ExpertSenderFr\\ExpertSenderApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ExpertSenderFr\\ExpertSenderApi\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/options-resolver": "^4.2|^5.0|^6.0",
    "symfony/serializer": "^4.2|^5.0|^6.0",
    "symfony/property-access": "^4.2|^5.0|^6.0",
    "symfony/dom-crawler": "^4.2|^5.0|^6.0",
    "psr/log": "^1.0|^2.0|^3.0",
    "ext-dom": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.5 <= 9.0",
    "roave/security-advisories": "dev-master"
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/"
  }
}
